{
  "name": "duplicate-and-paste",
  "displayName": "Duplicate & Paste",
  "description": "",
  "version": "1.0.0",
  "publisher": "JasonGmitter",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "duplicate-and-paste.DuplicateAndPaste",
        "title": "Duplicate & Paste"
      }
    ]
  },
  "scripts": {
